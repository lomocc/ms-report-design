      ],
      "data_bind": [
        {
          "value": "line_name"
        },
        {
          "value": "year_total"
        },
        {
          "value": "month_total"
        },
        {
          "value": "week_total"
        },
        {
          "value": "huan_zhou"
        },
        {
          "value": "w_yidai"
        },
        {
          "value": "w_qimiao"
        },
        {
          "value": "w_zz"
        },
        {
          "value": "w_cs"
        },
        {
          "value": "time_jindu"
        },
        {
          "value": "jixiao_jindu"
        },
        {
          "value": "year_target"
        },
        {
          "value": "total"
        }
      ]
